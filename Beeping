void setup() {
 pinMode(0, OUTPUT);
 pinMode(1, OUTPUT);
}
void loop() {
    digitalWrite(0, HIGH);
    digitalWrite(1, HIGH);
    delay(300);
    digitalWrite(0, LOW);
    delay(3250);
    digitalWrite(1,LOW);
    delay(3250);
    digitalWrite(1,HIGH);
    delay(3250);
    digitalWrite(1,LOW);
    delay(3250);  
    digitalWrite(1,HIGH);
    delay(3250);
    digitalWrite(1,LOW);
    delay(3250); 
    digitalWrite(1,HIGH);
    delay(3250);
    digitalWrite(1,LOW);
    delay(3250); 
}
